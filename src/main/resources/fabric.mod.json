{
	"schemaVersion": 1,

	"id": "discordrelay",
	"version": "${ version }",

	"name": "Discord Relay",
	"description": "Relay chat messages between Minecraft & Discord.",
	"authors": [ {
		"name": "viral32111",
		"contact": {
			"homepage": "https://viral32111.com",
			"email": "contact@viral32111.com"
		}
	} ],
	"contributors": [],
	"contact": {
		"homepage": "https://github.com/viral32111/discord-relay",
		"sources": "https://github.com/viral32111/discord-relay",
		"issues": "https://github.com/viral32111/discord-relay/issues"
	},
	"license": "AGPL-3.0-only",

	"environment": "server",
	"entrypoints": {
		"server": [ {
			"adapter": "kotlin",
			"value": "com.viral32111.discordrelay.DiscordRelay"
		} ]
	},
	"mixins": [ {
		"environment": "server",
		"config": "discordrelay.mixins.json"
	} ],

	"depends": {
		"java": ">=${ java }",
		"minecraft": ">=${ minecraft }",
		"fabricloader": ">=${ fabricloader }",
		"fabric-api": ">=${ fabric_api }",
		"fabric-language-kotlin": ">=${ fabric_language_kotlin }",
		"events": ">=${ events }"
	},
	"suggests": {}

}
